{
  "private": true,
  "name": "zhe-dev",
  "description": "Code of zhe.dev",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "build:remix": "remix build",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "postinstall": "remix setup cloudflare-pages",
    "dev:remix": "remix watch",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "dev:wrangler": "wrangler pages dev ./public --binding $(cat .env)",
    "start": "npm run dev:wrangler"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.4.2",
    "@remix-run/cloudflare-pages": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.23.2",
    "@sanity/image-url": "^1.0.1",
    "date-fns": "^2.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "twemoji": "^13.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/twemoji": "^12.1.2",
    "autoprefixer": "^10.0.2",
    "cross-env": "^7.0.3",
    "esbuild": "0.13.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.9",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.1.2",
    "wrangler": "^0.0.15"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
